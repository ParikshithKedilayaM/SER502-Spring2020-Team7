begin
	var x := 20;
	var xyz := 10;
	var abcdef;
	var y := 4;
	if(x+y==y+xyz)
		then x:=x+1;
	else y:=y+1;
	endif;
	for(i:=1;i<10;i:=i+1)
		do
		 if(x+y==y+xyz)
			then x:=x+1;
		else y:=y+1;
		endif;
	endfor;
	while(x!=y)
		do if(x+y==y+xyz)
			then x:=x+1;
		else y:=y+1;
		endif;
	endwhile;
	print(y);
end
